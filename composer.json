{
    "name": "coolsam/laravel-mongodb-world",
    "description": "Provide countries, states, and cities relations database for mongodb in laravel using jenssegers/laravel-mongodb",
    "keywords": [ "world", "country", "city", "region", "locale", "mongodb","laravel", "laravel-mongodb"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "laravel/framework" : ">=5.3.0",
        "jenssegers/mongodb": "^3.4",
        "jenssegers/mongodb-session": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
    	"psr-4": {
    		"Coolsam\\World\\": "src/"
    	}
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sam (coolsam) Maosa",
            "email": "maosa.sam@gmail.com"
        }
    ]
}
